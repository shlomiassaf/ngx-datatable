{
  "name": "@swimlane/ngx-datatable",
  "version": "4.1.0",
  "description": "ngx-datatable is a Angular2+ table grid component for presenting large and complex data.",
  "scripts": {
    "build:aot:prod": "npm run clean:compile && npm run ngc -- --webpack config/webpack.prod.js && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot:prod:imposter": "npm run clean:compile && npm run package && npm run clean:aot && npm run ngc:imposter -- --webpack config/webpack.prod.js && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "build:dev": "npm run clean:compile && webpack --config config/webpack.dev.js --progress --profile",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "npm run clean:compile && npm run ngc -- --webpack config/webpack.prod.js && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "changelog": "node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "ci:aot": "npm run lint && npm run test && npm run build:aot && npm run e2e",
    "ci:jit": "npm run lint && npm run test && npm run build:prod && npm run e2e",
    "ci:nobuild": "npm run lint && npm test && npm run e2e",
    "ci:testall": "npm run test && npm run build:prod && npm run e2e && npm run build:aot:prod:imposter && npm run e2e",
    "ci": "npm run ci:testall",
    "clean:compile": "npm run clean:dist && npm run clean:aot",
    "clean:aot": "npm run rimraf -- src/compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled",
    "docker": "docker",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm-run-all -p -r server:prod:ci e2e:only",
    "e2e:only": "npm run protractor",
    "github-deploy:dev": "npm run clean:dist && webpack --config config/webpack.github-deploy.js --progress --profile",
    "github-deploy:prod": "npm run clean:compile && npm run ngc -- --webpack config/webpack.prod.js && webpack --config config/webpack.github-deploy.js --progress --profile",
    "github-deploy": "npm run github-deploy:dev",
    "lint": "npm run tslint \"src/lib/**/*.ts\"",
    "ngc": "./node_modules/.bin/ngc-w -p tsconfig.webpack.json",
    "ngc:imposter": "./node_modules/.bin/ngc-w -p tsconfig.webpack.e2e-imposter.json",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "preversion": "npm test",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "package": "npm run rimraf -- dist_package && npm run package:ngc && webpack --config config/webpack.package.js && npm run package:rollup && npm run package:rollup:ts && npm run package:webpack:minify && npm run package:rollup:minify",
    "package:ngc": "npm run clean:aot && ./node_modules/.bin/ngc-w -p tsconfig.package.json --config config/webpack.prod.js",
    "package:rollup": "./node_modules/.bin/rollup -c rollup.config.js",
    "package:rollup:ts": "./node_modules/.bin/tsc --out dist_package/bundle/ngx-datatable.rollup.ts.umd.js --sourcemap --target es5 --allowJs --lib es2015,dom dist_package/bundle/ngx-datatable.rollup.umd.js",
    "package:rollup:minify": "./node_modules/.bin/uglifyjs dist_package/bundle/ngx-datatable.rollup.umd.js -m -c --screw-ie8 -o dist_package/bundle/ngx-datatable.rollup.umd.min.js --source-map dist_package/bundle/ngx-datatable.rollup.umd.min.js.map --source-map-url dist_package/bundle/ngx-datatable.rollup.umd.js.map",
    "package:webpack:minify": "./node_modules/.bin/uglifyjs dist_package/bundle/ngx-datatable.umd.js -m -c --screw-ie8 -o dist_package/bundle/ngx-datatable.umd.min.js --source-map dist_package/bundle/ngx-datatable.umd.min.js.map --source-map-url dist_package/bundle/ngx-datatable.umd.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/ngx-datatable.git"
  },
  "keywords": [
    "angularjs",
    "angular",
    "javascript",
    "angular2",
    "datatable",
    "grid",
    "table"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/ngx-datatable/issues"
  },
  "homepage": "https://github.com/swimlane/ngx-datatable#readme",
  "dependencies": {
    "@angular/common": "~2.4.1",
    "@angular/compiler": "~2.4.1",
    "@angular/core": "~2.4.1",
    "@angular/platform-browser": "~2.4.1",
    "@angular/platform-browser-dynamic": "~2.4.1",
    "@angular/router": "~3.4.1",
    "core-js": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "rxjs": "^5.0.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~2.4.1",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.2.34",
    "@types/node": "^6.0.38",
    "@types/selenium-webdriver": "2.53.33",
    "@types/source-map": "^0.1.27",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^2.0.0",
    "angular-restyle-loader": "^1.0.1",
    "angular2-template-loader": "^0.6.0",
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "~3.0.0-beta.17",
    "codelyzer": "^2.0.0-beta.3",
    "conventional-changelog-cli": "^1.2.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.21.0",
    "http-proxy-middleware": "^0.17.2",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "ng-router-loader": "^1.0.0",
    "ngc-webpack": "^1.0.6",
    "node-sass": "^3.10.1",
    "npm-run-all": "^4.0.0",
    "parse5": "^2.2.2",
    "postcss-loader": "^1.2.0",
    "postcss-merge-rules": "2.0.5",
    "postcss-sort-style-rules": "^0.2.0",
    "protractor": "^4.0.10",
    "raw-loader": "0.5.1",
    "request": "^2.78.0",
    "rimraf": "~2.5.4",
    "rollup": "^0.40.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "sass-loader": "^4.0.2",
    "sass-resources-loader": "^1.1.0",
    "script-ext-html-webpack-plugin": "^1.3.2",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "1.1.2",
    "ts-node": "^1.7.0",
    "tslint": "4.0.2",
    "tslint-config-swimlane": "^2.0.0",
    "tslint-loader": "^3.3.0",
    "typescript": "2.0.10",
    "uglify-js": "^2.7.5",
    "url-loader": "^0.5.7",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "~1.0.1"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
